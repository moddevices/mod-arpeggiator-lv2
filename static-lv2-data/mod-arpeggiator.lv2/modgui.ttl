@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mod-devel/arpeggiator>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-arpeggiator.html> ;
        modgui:stylesheet <modgui/stylesheet-arpeggiator.css> ;
        modgui:screenshot <modgui/mod_arpeggiator_ss.png> ;
        modgui:thumbnail <modgui/mod_arpeggiator_tn.png> ;
        modgui:brand "MOD" ;
        modgui:label "Arpeggiator" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "sync" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "Bpm" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "Divisions" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "velocity" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "noteLength" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "octaveSpread" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "arpMode" ;
        ] , [
            lv2:index 7 ;
            lv2:symbol "octaveMode" ;
        ] , [
            lv2:index 8 ;
            lv2:symbol "latch" ;
        ] ;
] .
